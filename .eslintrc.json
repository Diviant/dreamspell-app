{
    "extends": "react-app",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "off",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars":[
            "warn"
        ],
        "no-console":"warn",
        "no-useless-constructor" :"off"
    }
}